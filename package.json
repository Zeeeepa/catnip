{
  "name": "catnip",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:cf": "pnpm run build && wrangler dev 2>&1 | tee /tmp/wrangler.log",
    "dev:cf:catctrl": "./container/bin/catctrl logs pnpm run build && ./container/bin/catctrl logs wrangler dev",
    "dev:cf:vite": "CLOUDFLARE_DEV=true vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.worker.json",
    "typecheck:worker": "tsc --noEmit -p tsconfig.worker.json",
    "lint": "eslint .",
    "format": "echo \"Global pretty disabled for now...\" #prettier --write .",
    "format:check": "prettier --check .",
    "format:changed": "{ git diff --cached --name-only; git diff --name-only; git ls-files --others --exclude-standard; } | grep -E '\\.(ts|tsx|js|jsx|json|css|md)$' | xargs prettier --write",
    "preview": "pnpm run build && vite preview",
    "deploy:prod": "pnpm run build && wrangler deploy --env production",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@cloudflare/containers": "^0.0.22",
    "@hono/oauth-providers": "^0.8.2",
    "@octokit/app": "^16.0.1",
    "@octokit/core": "^7.0.3",
    "@octokit/webhooks": "^14.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-router": "^1.125.4",
    "@tanstack/react-router-devtools": "^1.125.4",
    "@types/prismjs": "^1.26.5",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "hono": "^4.8.4",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-xtermjs": "^1.0.10",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.9.4",
    "@cloudflare/workers-types": "^4.20250712.0",
    "@eslint/js": "^9.29.0",
    "@inquirer/prompts": "^7.6.0",
    "@tanstack/router-plugin": "^1.125.5",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-dom": "^1.52.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-react-x": "^1.52.2",
    "globals": "^16.2.0",
    "prettier": "^3.6.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "wrangler": "^4.25.0"
  }
}
